<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>LoloBnB{% block title %}Welcome!{% endblock %}</title>
        {{ encore_entry_link_tags('app') }}
        {{ encore_entry_link_tags('dashboard') }}
        <script src="https://kit.fontawesome.com/b5ab9226f9.js" crossorigin="anonymous"></script>
        {% block stylesheets %}{% endblock %}
    </head>
    <body>
        {% include 'admin/partials/header.html.twig' %}
        <main role="main" class="col-md-9 ml-sm-auto col-lg-10 px-md-4 py-4">
        {% block body %}{% endblock %}
        </main>
        {% include 'admin/partials/footer.html.twig' %}
        {{ encore_entry_script_tags('app') }}
        
        <script src="https://cdnjs.cloudflare.com/ajax/libs/feather-icons/4.9.0/feather.min.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.7.3/Chart.min.js"></script>
        {{ encore_entry_script_tags('dashboard') }}
        {% block javascripts %}{% endblock %}
        
    </body>
</html>
